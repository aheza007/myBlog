@model SimpleBlog.Areas.Admin.ViewModels.UsersIndex
@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>User</title>
   
</head>
<body>
    <div>
        <h1>USERs</h1>
        <ul>
            @foreach(var user in Model.Users){
                
                <li>@user.Username</li>
            
            }

        </ul>
         
    </div>
</body>
</html>
